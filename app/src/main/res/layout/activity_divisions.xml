<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Divisions">


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/button2"
        android:layout_width="54dp"
        android:layout_height="73dp"
        android:layout_gravity="bottom|right"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:contentDescription="fab_content_desc"
        android:scaleType="centerInside"
        android:src="@drawable/arrow_down_foreground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="VectorDrawableCompat"
        android:elevation="10dp"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Bills_fab"
        android:layout_width="54dp"
        android:layout_height="73dp"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:contentDescription="fab_content_desc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.382"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="VectorDrawableCompat" />

    <!--<com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/track"
        android:layout_width="54dp"
        android:layout_height="73dp"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:contentDescription="fab_content_desc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.395"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.902"
        tools:ignore="VectorDrawableCompat" />-->

    <Button
        android:id="@+id/Trigger_button"
        android:layout_width="153dp"
        android:layout_height="165dp"
        android:background="@drawable/ic_button_icon_red"
        android:gravity="center"
        android:text="Trigger"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_error"
        app:backgroundTint="#F44336"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.178"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Diary_button"
        android:layout_width="161dp"
        android:layout_height="191dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/ic_baseline_book_24"
        android:gravity="center"
        android:text="                         Diary"
        android:textColor="@color/white"
        app:backgroundTint="#03A9F4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@id/Trigger_button"
        app:layout_constraintTop_toBottomOf="@id/header" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="260dp"
        android:layout_height="46dp"
        android:fontFamily="monospace"
        android:text="Other Functionalities "
        android:textColor="@color/com_smart_login_code"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Trigger_button"
        app:layout_constraintVertical_bias="0.379" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Button"
        android:layout_width="54dp"
        android:layout_height="73dp"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:contentDescription="fab_content_desc"
        android:src="@drawable/arrow_down_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.329"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.797"
        tools:ignore="VectorDrawableCompat" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Track_fab"
        android:layout_width="54dp"
        android:layout_height="73dp"
        android:layout_gravity="bottom|right"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="12dp"
        android:contentDescription="fab_content_desc"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/Button"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/Track_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Track"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/Track_fab"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintBottom_toTopOf="@+id/Bills_text"/>

    <TextView
        android:id="@+id/Bills_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:text="Bills/Reports"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/Bills_fab"
        app:layout_constraintTop_toBottomOf="@+id/Track_text" />

    <Button
        android:id="@+id/logout_divisions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:text="LogOut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        android:elevation="10dp"/>

    <TextView
        android:layout_width="411dp"
        android:layout_height="179dp"
        android:text="Dashboard"
        android:textSize="40sp"
        android:textColor="#fafafa"
        android:background="@drawable/header_bg"
        android:gravity="center_vertical|bottom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:padding="12dp"
        android:id="@+id/header"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="220dp"
        android:layout_marginLeft="220dp"
        android:layout_marginBottom="4dp"
        android:text="for Business"
        android:textColor="#fafafa"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@id/header"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/BrowserV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Browser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logout_divisions" />
</androidx.constraintlayout.widget.ConstraintLayout>
